{
  "name": "jimu-control",
  "version": "0.0.1",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:ui\" \"npm:dev:electron\"",
    "dev:ui": "vite",
    "dev:electron": "set ELECTRON_RUN_AS_NODE=&& set ELECTRON_DEV=true&& set NODE_ENV=development&& wait-on http://localhost:5173 && electron .",
    "build": "vite build",
    "start": "set ELECTRON_RUN_AS_NODE=&& electron ."
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-15",
    "@radix-ui/react-slider": "^1.3.6",
    "blockly": "^10.4.3",
    "nipplejs": "^0.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^2.1.0",
    "react-resizable": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.0",
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "vite": "^5.2.12",
    "wait-on": "^7.2.0"
  }
}
