{
  "name": "jimu-control",
  "version": "0.0.1",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:ui\" \"npm:dev:electron\"",
    "dev:ui": "vite",
    "dev:electron": "set ELECTRON_DEV=true&& set NODE_ENV=development&& wait-on http://localhost:5173 && electron .",
    "build": "vite build",
    "start": "electron ."
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.0",
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "wait-on": "^7.2.0",
    "vite": "^5.2.12"
  }
}
